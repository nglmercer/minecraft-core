{
  "name": "minecraft-core",
  "version": "1.0.0",
  "description": "Core library for Minecraft-related utilities",
  "keywords": [
    "minecraft",
    "core",
    "utilities",
    "typescript"
  ],
  "author": "nglmercer",
  "license": "MIT",
  "homepage": "https://github.com/nglmercer/minecraft-core#readme",
  "bugs": {
    "url": "https://github.com/nglmercer/minecraft-core/issues"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nglmercer/minecraft-core.git"
  },
  "scripts": {
    "test": "bun test",
    "manual": "bun run tests/manual.ts",
    "typecheck": "bunx tsc --noEmit",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "bun run build",
    "publish": "npm publish"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^8.5.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {}
}